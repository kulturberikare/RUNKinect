<Window x:Class="NewTreadmillAngleSpeedAB.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="1080" Width="1920">
    <Viewbox>
        <Grid>
            <StackPanel VerticalAlignment="Top">
                <TextBlock x:Name="AngleText" FontFamily="Arial Black" FontSize="48" HorizontalAlignment="Right"/>
            </StackPanel>
            <StackPanel VerticalAlignment="Top">
                <TextBlock x:Name="SpeedText" FontFamily="Arial Black" FontSize="48" HorizontalAlignment="Left"/>
            </StackPanel>
        </Grid>
    </Viewbox>
</Window>
